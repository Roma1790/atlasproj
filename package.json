{
  "name": "atlas",
  "version": "0.0.0-development",
  "description": "Mapping library for jobs",
  "homepage": "https://github.com/chronark/atlas",
  "bugs": {
    "url": "https://github.com/chronark/atlas/issues"
  },
  "repository": "https://github.com/chronark/atlas.git",
  "license": "MIT",
  "author": "Andreas Thomas <andreas.thomas@th-nuernberg.de>",
  "scripts": {
    "build": "webpack --mode=production --progress",
    "build:docs": "./node_modules/.bin/typedoc",
    "buildandserve": "npm run build && npm run serve",
    "dev": "webpack-dev-server --progress",
    "docker": "docker build -t chronark/atlas . && docker run --rm -p 80:80 chronark/atlas",
    "fmt": "npm run prettier && npm run lint",
    "lint": "eslint --fix --ext .js,.ts ./",
    "prettier": "prettier --write ./**/*.{js,ts,json} --loglevel warn",
    "serve": "serve ./dist",
    "test:e2e": "node ./__tests__/e2e/testcafe.js",
    "test:full": "npm install && npm run fmt && npm run tsc && npm run test && npm run build && export CI=true && npm run test:e2e",
    "test": "jest",
    "tsc": "npm run tsc:default && npm run tsc:testcafe",
    "tsc:testcafe": "npm run tsc:default -- -p ./tsconfig.testcafe.json",
    "tsc:default": "./node_modules/.bin/tsc --noEmit --pretty",
    "updateJobs": "curl https://jobboerse.th-nuernberg.de/srv.php/en/Suche/offers > ./static/rawJobs.json && prettier --write ./static/rawJobs.json",
    "typedoc": "./node_modules/.bin/typedoc"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "ol": "^6.9.0",
    "ol-ext": "^3.2.16",
    "ol-mapbox-style": "^6.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-for-of": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/geojson": "^7946.0.8",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/ol": "^6.5.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@undefinedlabs/scope-agent": "0.7.4",
    "babel-loader": "^8.2.3",
    "core-js": "^3.19.2",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsdoc": "^37.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testcafe": "0.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "proxy-polyfill": "0.3.2",
    "regenerator-runtime": "^0.13.9",
    "serve": "^13.0.2",
    "source-map-loader": "^3.0.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "testcafe": "^1.17.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.7",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "main": "index.js"
}
