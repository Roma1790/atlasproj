{
  "name": "pantheon",
  "version": "0.0.0-development",
  "description": "Mapping library for jobs",
  "homepage": "https://github.com/chronark/pantheon",
  "bugs": {
    "url": "https://github.com/chronark/pantheon/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chronark/pantheon.git"
  },
  "license": "MIT",
  "author": "Andreas Thomas <andreas.thomas@th-nuernberg.de>",
  "main": "dist/pantheon.js",
  "scripts": {
    "build": "rm -rf ./dist/ && run-p --print-label build-src build-dts",
    "build--watch": "onchange 'src/**/*' --initial --kill --delay 1000  -- npm run build",
    "build-dts": "tsc --emitDeclarationOnly",
    "build-dts--watch": "npm run build-dts -- --watch --preserveWatchOutput",
    "build-src": "babel src --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build-src--watch": "npm run build-src -- --watch 'src/**/*' --verbose",
    "dev": "NODE_ENV=development npm run start-src--watch",
    "dev--debug": "DEBUG=pantheon npm run dev",
    "display-browser-support": "browserslist --stats && echo '' && browserslist --coverage && echo '' && echo 'See more at https://browserl.ist/'",
    "format": "run-s format-package-json format-prettier format-eslint",
    "format-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --fix ./ >/dev/null 2>&1 || true",
    "format-package-json": "sort-package-json",
    "format-prettier": "prettier --write ./**/*.{js,ts,json}",
    "lint": "run-p lint-eslint",
    "lint--watch": "onchange 'src/**/*' --initial --kill --delay 1000  -- npm run lint",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "start": "webpack-dev-server --open",
    "start--dev": "NODE_ENV=development npm run start",
    "start-src": "babel-node --extensions .js,.jsx,.ts,.tsx --require node_modules/dotenv/config src/pantheon.ts",
    "start-src--watch": "nodemon --ext js,jsx,ts,tsx --delay 1 --exec npm run start-src",
    "test": "jest",
    "test--coverage": "npm run test -- --coverage",
    "test--watch": "npm run test -- --watch",
    "test:e2e": "cypress open",
    "test:e2e:ci": "cypress run",
    "typecheck": "tsc --noEmit",
    "typecheck--watch": "npm run typecheck -- --watch",
    "validate": "run-p --print-label lint typecheck test build",
    "webpack": "webpack --mode=development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@typescript-eslint/parser": "^2.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "cypress": "^3.4.1",
    "dotenv": "^7.0.0",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "ts-jest": "^24.0.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "browserslist": "^4.5.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^1.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-unicorn": "^8.0.1",
    "husky": "^1.3.1",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^24.7.0",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.2",
    "sort-package-json": "^1.22.1",
    "typescript": "^3.4.1"
  }
}
