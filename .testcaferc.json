{
  "browsers": "all",
  "src": "./__tests__/e2e/*.test.js",
  "takeScreenshotsOnFails": true,
  "screenshotPath": "./__tests__/e2e/reports/screenshots/",
  "screenshotPathPattern": "${DATE}_${TIME}/test-${TEST_INDEX}/${USERAGENT}/${FILE_INDEX}.png",
  "stopOnFirstFail": true,
  "concurrency": 1,
  "reporter": "list",
  "color": true,
  "skipJsErrors": false
}
